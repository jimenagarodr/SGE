    "cont_dentro = 0\n",
    "fig = plt.figure(figsize=(8,8))\n",
    "ini = -1\n",
    "fin = 1\n",
    "xLista = []\n",
    "yLista = []\n",
    "colores= []\n",
    "# ax = fig.add_subplot(111)\n",
    "\n",
    "for i in range (N):\n",
    "    x = 1-2*random.random()\n",
    "    y = 1-2*random.random()\n",
    "    xLista.append(x)\n",
    "    yLista.append(y)\n",
    "    \n",
    "if (x*x + y*y <= 1):\n",
    "    colores.append(\"magenta\")\n",
    "    cont_dentro+=1\n",
    "else :\n",
    "    colores.append(\"blue\")\n",
    "\n",
    "plt.scatter(xLista, yLista, c=colores, s=0.5)\n",
    "plt.show()\n",
    "\n",
    "miPi = 4*cont_dentro / N\n",
    "print(\"El valor de 𝜋 es \" + str(miPi) + \"?\")"
